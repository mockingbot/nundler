  "trimGz": true,
  "upload": true,
  "urlFileUpload": "http://localhost:18080/file-chunk-upload",
  "uploadDirectory": "./sample-gz-directory-gitignore",
  "uploadKey": "test-upload/test-gz-directory-{git-branch}-{git-commit-hash}.tgz",
  "authFile": "./example-gitignore.key"
}
