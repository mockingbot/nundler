{
  "private": true,
  "name": "nundler",
  "version": "0.0.1-dev.1",
  "description": "Not bundler",
  "author": "mockingbot",
  "license": "MIT",
  "bin": "bin/index.js",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node script quiet",
    "script-pack": "babel-node script quiet pack",
    "script-pack-test": "babel-node script pack test",
    "script-publish": "babel-node script pack publish",
    "script-publish-dev": "babel-node script pack publish-dev",
    "script-generate-spec": "babel-node script/generateSpec",
    "script-test-example": "babel-node script/testExample",
    "// build =======================": "",
    "build-library": "babel-node script/webpack production profile",
    "build-library-dev": "babel-node script/webpack development watch",
    "// test ========================": "",
    "example-reset": "babel-node example/index.js clear init",
    "start-example-server": "npx dr-server -c example/server.config.json",
    "test-list": "node output-gitignore/bin -c example/test-list.config.json",
    "test-upload": "node output-gitignore/bin -c example/test-upload.config.json",
    "test-download": "node output-gitignore/bin -c example/test-download.config.json",
    "test-package-download": "node output-gitignore/bin -c example/test-package-download.config.json",
    "// =============================": "",
    "analyze-stat": "npx webpack-bundle-analyzer .temp-gitignore/profile-stat.json",
    "test": "npm run script-pack-test",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "engines": {
    "node": ">=10.12",
    "npm": ">=6"
  },
  "devDependencies": {
    "dr-dev": "0.0.1",
    "dr-dev-web": "0.0.1",
    "dr-js": "^0.18.2",
    "dr-server": "^0.0.28"
  }
}
